            </div>
            <div class="col l4 m6 s12">
                <img class="card responsive-img materialboxed" src="~/lib/img/main/background1.jpg">
            </div>
        </div>
    </div>
</div>

<div class="parallax-container valign-wrapper">
    <div id="contact" class="section no-pad-bot">
        @await Component.InvokeAsync("ContactFormComponent")
    </div>
    <div class="parallax"><img src="~/lib/img/main/background2blur.jpg" alt="Unsplashed background img 3"></div>
