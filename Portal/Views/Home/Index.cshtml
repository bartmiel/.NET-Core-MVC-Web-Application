@{
    ViewData["Title"] = "Strona główna";
}
<div class="container">
    <!--   Blog Section   -->
    <div class="section">
        @await Html.PartialAsync("Partial/_BlogSection", (Portal.Models.DtoModels.BlogSectionDto)ViewBag.BlogSection)
    </div>
</div>

<div class="parallax-container valign-wrapper counter-section">
    <!--   Counter Section   -->
    @await Html.PartialAsync("Partial/_CounterSection", (Portal.Models.DtoModels.ParametersDto)ViewBag.Parameters)
    <div class="parallax"><img src="~/lib/img/main/background6.jpg" alt="Unsplashed background img 2"></div>
</div>

<div class="container">
    <!--   About Us Section   -->
    @await Html.PartialAsync("Partial/_AboutUsSection", (Portal.Models.DtoModels.ParametersDto)ViewBag.Parameters)
</div>

